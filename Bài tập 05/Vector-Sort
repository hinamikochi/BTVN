#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */   int a,b;
    cin >> a;
    vector<int> v;
    
    for(int i=0; i<a; i++){
        cin>>b;
        v.push_back(b);
    }
    sort(v.begin(),v.end());
    
    for(int i=0; i<a; i++){
        cout<<v[i]<<" ";
    }
    return 0;
}

